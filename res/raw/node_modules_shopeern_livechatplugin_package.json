{
  "name": "@shopee-rn/livechat-plugin",
  "version": "0.1.21",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "LiveChatSDK.d.ts",
  "files": [
    "src/",
    "assets/",
    "LiveChatSDK.d.ts"
  ],
  "scripts": {
    "api": "yarn ytt",
    "precommit": "rn-cli validate precommit && lint-staged",
    "dev": "hostless-dev-server start",
    "dev:chatbot": "cd ../web-chatbot-rn && yarn dev",
    "dev-host": "rn-cli dev",
    "release": "rn-cli release",
    "validate": "rn-cli validate",
    "lint": "eslint --ext .js,.ts,.tsx ./src",
    "type:check": "tsc --noEmit --skipLibCheck",
    "generate-svg": "node script/getSvg.js",
    "tracking": "cd src/tracking && node ../../node_modules/@shopee/tracking-conversion/cli.js bundle . dist",
    "tracking:dev": "cd src/tracking && node ../../node_modules/@shopee/tracking-conversion/cli.js dev . dist",
    "tsp-transify": "node script/tsp-transify.js",
    "transify-bank": "fetch-transify --mapping=./transify_mapping_bank.js --sorted --fallback"
  },
  "devDependencies": {
    "@cpp/codepush-cli": "^6.1.4",
    "@react-native-community/eslint-config": "^3.2.0",
    "@shopee-rn/hostless-dev-server": "^2.4.2",
    "@shopee-rn/optimize-images": "^1.1.7",
    "@shopee/fetch-assets": "^0.1.29",
    "@shopee/fetch-transify": "^0.0.18",
    "@shopee/react-native-sdk": "^3.79.0",
    "@shopee/rn-cli": "latest",
    "@shopee/ytt-plugin-inhouse-preset": "^0.13.0",
    "@types/md5": "^2.3.2",
    "@types/react": "^16.13.1",
    "@types/react-native": "^0.63",
    "axios": "^0.21.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ft-flow": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^2.8.7",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "16.8.4",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@shopee-rn/app-env": "^2.9.1",
    "@shopee/react-native-sdk": "^3.79.0",
    "react": "16.13.1",
    "react-native": "0.63.3"
  },
  "dependencies": {
    "@lego/utils-echo-channel": "0.0.2-beta-20240711161703",
    "@echo/monitor": "^0.0.7",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/netinfo": "4.7.0",
    "@shopee-rn/csc-order-modal": "^1.0.1",
    "@shopee-rn/nebula": "^3.8.1",
    "@shopee-rn/tracking-components": "^4.2.1",
    "@shopee-rn/ui-common": "^2.6.11",
    "@shopee/chatbot-ui": "1.0.53-alpha.211",
    "@shopee/get-v2-time-locale": "^2.3.0",
    "@shopee/livechat-utils": "^0.1.3-beta.2",
    "@shopee/tracking-conversion": "^1.2.19",
    "@shopee/yapi-to-typescript": "^1.0.2",
    "@shopee_common/time": "^2.3.1",
    "@shopee_node/isomorphic-request": "^2.5.0",
    "axios": "^0.22.0",
    "dayjs": "^1.11.9",
    "events": "^3.3.0",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "react-native-svg": "9.13.6",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.1",
    "tsp-rn-sdk": "^0.0.18"
  },
  "resolutions": {
    "#": "Prevents using conflicting versions of types for react",
    "@types/react": "^16.13.1",
    "axios": "^0.22.0",
    "swagger-client": "3.18.4",
    "@shopee/react-native-sdk": "^3.79.0",
    "react-native-svg": "9.13.6",
    "**/mockjs/commander": "^7.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx,json}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{png,jpg,jpeg,gif}": [
        "shopee-rn-image-optimize",
        "git add"
      ]
    }
  }
}
